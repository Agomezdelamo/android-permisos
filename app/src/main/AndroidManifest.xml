<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="seccion01.android.agomez.seccion_01">
    <!--
        <permission>se usa normalmente al hacer un permiso personalizado
        (por ejemplo, cuando se crea una aplicación a la que otras aplicaciones pueden vincularse,
        limitar el acceso es obligatorio) y
        <uses-permission>se usa cuando su aplicación realmente necesita un permiso que normalmente no tiene.
    -->

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".actividades.SecondActivity" />
        <activity android:name=".ThirdActivity"></activity>
    </application>

</manifest>